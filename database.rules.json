{
  "rules": {
    ".read": "root.child('admins').child(auth.uid).val() === true",
    "problems": {
      "$problemId": {
        ".read": "true"
      }
    },
    "data": {
      "$problemId": {
        ".read": "false"
      }
    },
    "contestants": {
      "$uid": {
        ".read": "$uid === auth.uid"
      }
    },
    "contest": {
      "info": {
        ".read": "true"
      },
      "solved": {
        "$uid": {
          ".read": "$uid === auth.uid"
        }
      },
      "points": {
        "$uid": {
          ".read": "$uid === auth.uid"
        }
      }
    },
    ".write": false
  }
}
